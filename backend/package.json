    "@types/node": "^20.12.7",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.11.6",
    "@types/uuid": "^9.0.8",
    "drizzle-kit": "^0.20.17",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.2"
